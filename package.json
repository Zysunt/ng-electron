{
  "name": "webel",
  "version": "0.0.0",
  "main": "main.js",
  "description": "webport",
  "author": {
    "name": "zoey"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:build": "npm run build --prod && electron-builder build",
    "electron:start": "ng build --base-href ./ && electron .",
    "win": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.zoey.app",
    "productName": "webport",
    "copyright": "jiangsuport",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "./src/assets/logo.svg"
    }
  },
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./src/assets/logo.svg",
    "uninstallerIcon": "./src/assets/logo.svg",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "zoey",
    "include": "build/script/installer.nsh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/forms": "~9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
